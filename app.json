{
  "expo": {
    "name": "SIH",
    "slug": "SIH",
    "version": "1.0.0",
    "orientation": "portrait",
    "assetBundlePatterns": [
      "**/*"
    ],
    "splash": {
      "image": "./src/images/neer_splash_bg.png",
      "resizeMode": "cover"
    },
    "icon": "./src/images/neer_icon_bg.png",
    "plugins": [
      [
        
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your community."
        }
      ]
    ],
    "android": {
      "package": "com.sam79.SIH",
      "config": {
        "googleMaps": {
          "apiKey": "Your_GOOGLE_MAPS_API_KEY"
        }
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "ACCESS_MEDIA_LOCATION"
      ]
    }
  }
}